package main;

import java.util.Scanner;

public class Driver {

	public static void main(String[] args) {
		int burstTime, waitTime, turnaroundTime, averageTurnTime = 0, averageWaitTime = 0, i, j;
		
		
		 try {
			System.out.println("Enter total number of processes (maximum 20): ");
			 Scanner number = new Scanner(System.in);
			 int n = number.nextInt();
		} catch (Exception e) {
			e.printStackTrace();
			
			for (i = 0; i < number; i++) {
				 System.out.println("");
			    }
		}
	}

}
